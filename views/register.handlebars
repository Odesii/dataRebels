<div id="register">
<div class="row justify-space-between-md">
  <div class="login-card">
    <h2 class="page-title">Signup</h2>
    <form class="form signup-form">
      <div class="form-group">
        <label for="username-signup">Username:</label>
        <input class="form-input" type="text" id="username-signup" />
      </div>
      <div class="form-group">
        <label for="password-signup">Password:</label>
        <input class="form-input" type="password" id="password-signup" />
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit">RABBIT HOLE</button>
      </div>
    </form>
  </div>
</div>
<section id="reg-split">
  <h2>Already have an account? Login instead!</h2>
  <a href="/login"><button>ONE OF US</button></a>
</section>
</div>
<script src="./js/login.js"></script>
<script type="module" src="./js/3dRender/base.js"></script>